{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "nodeModulesPath": ".",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "large"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "nodeModulesPath": ".",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "large",
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "nodeModulesPath": ".",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "large"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
